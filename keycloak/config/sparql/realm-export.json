{
  "realm": "sparql",
  "enabled": true,
  "roles": {
    "realm": [
      {
        "name": "doctor",
        "description": "User with access to protected query endpoint",
        "composite": false,
        "clientRole": false
      }
    ]
  },
  "clients": [
    {
      "clientId": "sparql-client",
      "secret": "secret123",
      "enabled": true,
      "protocol": "openid-connect",
