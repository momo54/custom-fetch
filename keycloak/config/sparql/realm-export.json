      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": true,
      "fullScopeAllowed": true,
      "attributes": {
        "access.token.lifespan": "3600"
      }
    }
  ],
  "users": [
    {
      "username": "alice",
      "enabled": true,
      "email": "alice@example.org",
      "emailVerified": true,
      "firstName": "Alice",
      "lastName": "Tester",
      "credentials": [
        {
          "type": "password",
          "value": "alicepwd",
          "temporary": false
        }
      ],
      "realmRoles": [
        "doctor"
      ]
    }
  ]
}